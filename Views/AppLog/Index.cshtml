@model IEnumerable<DAHAR.Models.AppLogModel>
@{
    ViewData["Title"] = "Application Logs";
    var iteration = 1;
}

<div class="col-12">
<div class="card">
    <div class="card-body">
        <table id="example2" class="table table-bordered table-hover">
            <thead>
            <tr>
                <th>No</th>
                <th>Date</th>
                <th>User</th>
            </tr>
            </thead>
            <tbody>
            @foreach(var item in Model)
            {
                <tr>
                    <td>@iteration</td>
                    <td>@item.CreatedAt</td>
                    <td>@item.CreatedBy</td>
                </tr>
                iteration++;
            }
            </tbody>
        </table>
    </div>
</div>
</div>

